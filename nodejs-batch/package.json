{
  "name": "ip-registry-data-processor",
  "version": "1.0.0",
  "description": "IP Registry Data Processing Batch System - Node.js Implementation",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "oxlint ./src --fix",
    "format": "prettier --write './src'",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/e2e",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "ip-registry",
    "ip-allocation",
    "batch-processing",
    "ipv4",
    "ipv6",
    "cidr"
  ],
  "author": "Akira345",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "eslint-plugin-security": "^4.0.0",
    "jest": "^29.7.0",
    "oxlint": "^1.49.0",
    "prettier": "^3.8.1"
  },
  "dependencies": {
    "cidr-tools": "^11.0.10",
    "dotenv": "^17.3.1",
    "mysql2": "^3.17.4",
    "undici": "^7.22.0",
    "yargs": "^18.0.0"
  },
  "engines": {
    "node": ">=24.13.1"
  },
  "bin": {
    "ip-processor": "src/index.js"
  }
}
